{
  "name": "ipfs-web-upload",
  "version": "0.1.0",
  "private": true,
  "description": "https://ipfs-web-upload.now.sh/",
  "keywords": [],
  "homepage": "https://github.com/bluelovers/ipfs-web-upload#readme",
  "bugs": {
    "url": "https://github.com/bluelovers/ipfs-web-upload/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bluelovers/ipfs-web-upload.git"
  },
  "license": "ISC",
  "author": "bluelovers",
  "main": "next.config.js",
  "scripts": {
    "start": "next start",
    "dev": "next dev -p 9999",
    "test": "echo \"Error: no test specified\"",
    "build": "next build",
    "build:export": "next build && next export",
    "build:ipfs": "yarn run build:export && esm-ts-node ./script/upload-to-ipfs.ts",
    "npm:publish": "npm publish",
    "npm:publish:lerna": "ynpx --quiet lerna -- publish --yes --bump patch",
    "prepublishOnly": "yarn run build:ipfs",
    "prepublishOnly:check-bin": "ynpx --quiet @yarn-tool/check-pkg-bin",
    "prepublishOnly:update": "yarn run ncu && yarn run sort-package-json",
    "prepublishOnly_": "yarn run prepublishOnly:check-bin && yarn run prepublishOnly:update && yarn run test",
    "postpublish:changelog": "ynpx --quiet @yarn-tool/changelog && git add ./CHANGELOG.md",
    "postpublish:git:commit": "git commit -m \"chore(release): publish\" . & echo postpublish:git:commit",
    "postpublish:git:push": "git push --follow-tags",
    "postpublish:git:tag": "ynpx --quiet @yarn-tool/tag",
    "postpublish": "yarn run postpublish:changelog && yarn run postpublish:git:commit && yarn run postpublish:git:tag && yarn run postpublish:git:push",
    "ci:install": "yarn add --disable-self-update-check --prefer-offline ipfs ipfsd-ctl @bluelovers/fast-glob fast-glob",
    "export": "next export",
    "ncu": "npx yarn-tool ncu -u && npx yarn-tool install && yarn run sort",
    "sort": "npx yarn-tool sort",
    "sort-package-json": "ynpx --quiet yarn-tool -- sort"
  },
  "dependencies": {
    "@bluelovers/ipfs-http-client": "^1.0.70",
    "@lazy-ipfs/buffer-json": "^1.0.3",
    "@lazy-ipfs/detect-ipfs-companion": "^1.0.9",
    "@lazy-ipfs/ipfs-protocol": "^1.0.2",
    "@lazy-ipfs/ipfs-subdomain": "^1.0.8",
    "@lazy-ipfs/to-cid": "^1.0.5",
    "@tanker/file-reader": "^2.4.0",
    "array-hyper-unique": "^1.1.10",
    "bluebird": "^3.7.2",
    "buffer": "^6.0.3",
    "cids": "^1.1.7",
    "consola": "^2.13.0",
    "cross-fetch": "^3.1.4",
    "fetch-ipfs": "^1.0.77",
    "ipfs-api-type": "^1.0.15",
    "ipfs-browser-util": "^1.0.5",
    "ipfs-core-types": "^0.5.2",
    "ipfs-http-client": "^44.2.0",
    "ipfs-server-list": "^1.0.54",
    "ipfs-util-lib": "^1.0.55",
    "is-ipfs": "^6.0.1",
    "next": "9.4.4",
    "poke-ipfs": "^1.1.13",
    "pretty-bytes": "^5.3.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-dropzone": "^11.0.1",
    "sass": "^1.26.8",
    "to-ipfs-url": "^1.0.25",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.32",
    "@types/node": "^14.0.13",
    "@types/react": "^16.9.36",
    "@types/react-dom": "^16.9.8",
    "esm": "^3.2.25",
    "esm-ts-node": "^1.0.5",
    "ts-node": "^8.10.2",
    "typescript": "^4.4.0-dev.20210714",
    "use-ipfs": "^1.0.59"
  }
}
